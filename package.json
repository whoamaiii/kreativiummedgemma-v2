{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs:lint": "markdownlint README.md docs/**/*.md --ignore docs/preprocessing/*.md",
    "docs:format": "prettier --write \"**/*.{md,mdx}\"",
    "docs:check": "prettier --check \"**/*.{md,mdx}\" && npm run docs:lint",
    "build:poc": "vite build --mode poc",
    "dev:poc": "vite --mode poc",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:poc": "vite preview --mode poc",
    "test": "vitest",
    "test:config": "vitest run src/config/__tests__/analytics.config.test.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:locales": "node scripts/lint-locales.js",
    "e2e:install": "npx --yes playwright@latest install --with-deps",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:headless": "playwright test --reporter=list",
    "e2e:a11y": "playwright test tests/e2e/a11y-smoke.spec.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:view": "lhci open",
    "reset-storage": "node scripts/reset-storage.js",
    "reset-storage:all": "node scripts/reset-storage.js --all",
    "reset-storage:dry": "node scripts/reset-storage.js --dry",
    "i18n:scan": "tsx scripts/i18n/scan.ts && tsx scripts/i18n/offenders_to_md.ts",
    "i18n:check": "tsx scripts/i18n/key_diff.ts",
    "i18n:report": "node -e \"const fs=require('fs');const cp=require('child_process');const p='artifacts/i18n-offenders.md';if(!fs.existsSync(p)){console.error('Missing '+p+'. Run npm run i18n:scan first.');process.exit(1);}try{const cmd=process.platform==='darwin'?'open':process.platform==='win32'?'start':'xdg-open';cp.execSync(cmd+' '+p,{stdio:'ignore'});}catch(e){fs.createReadStream(p).pipe(process.stdout);}\"",
    "precommit:lint": "lint-staged",
    "prepare": "husky install",
    "test:performance": "vitest run src/lib/testing/performanceTester.test.ts",
    "test:bias": "vitest run src/lib/testing/biasTester.test.ts",
    "analyze": "npm run build && npx http-server artifacts -p 3001 -o /bundle-stats.html",
    "prepare-local-pr": "node scripts/verify-local-pr.js",
    "seed:demo": "tsx scripts/seed-demo.ts",
    "unused-exports": "node scripts/find-unused-exports.js",
    "cleanup": "npm run lint:fix && npm run unused-exports",
    "lint:unused": "eslint -c .eslintrc.unused-exports.js ."
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^9.86.13",
    "@react-three/fiber": "^8.18.0",
    "@tanstack/react-query": "^5.56.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/three": "^0.178.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^12.23.11",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.452.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^6.26.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.178.0",
    "xxhashjs": "^0.2.2",
    "zod": "3.25.76"
  },
  "overrides": {
    "esbuild": ">=0.24.3",
    "d3-color": ">=3.1.0",
    "node-fetch": ">=2.6.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@browser-echo/vite": "^0.0.6",
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "markdownlint": "^0.33.0",
    "markdownlint-cli": "^0.41.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "eslint-plugin-unused-imports": "^4.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  }
}
